{
  "expo": {
    "name": "parkrunapps",
    "slug": "parkrunapps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/parkrunicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thairunapps.parkrunapps",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.thairunapps.parkrunapps",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/parkrunicon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "fonts": [
      {
        "asset": "assets/fonts/NotoSansThai-Regular.ttf",
        "family": "NotoSansThai-Regular"
      }
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/parkrunicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-barcode-scanner"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4a87ed36-b1f3-4858-a779-acc594176533"
      }
    }
  }
}
